{
  "name": "@botbuildercommunity/storage-mssql",
  "version": "1.0.0",
  "description": "Microsoft SQL Server storage option for the Microsoft Bot Framework",
  "keywords": [
    "botbuilder",
    "bots",
    "chatbots",
    "mssql",
    "microsoft sql server",
    "storage"
  ],
  "contributors": [
    "Bot Builder Community"
  ],
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/BotBuilderCommunity/botbuilder-community-js/issues"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/BotBuilderCommunity/botbuilder-community-js.git"
  },
  "nyc": {
    "extends": "@istanbuljs/nyc-config-typescript",
    "all": true,
    "check-coverage": true,
    "lines": 0,
    "statements": 0,
    "functions": 0,
    "branches": 0,
    "exclude": [
      "**/*.d.ts",
      "**/*.test.js"
    ],
    "reporter": [
      "text",
      "cobertura",
      "html"
    ],
    "report-dir": "./.nyc_output/coverage"
  },
  "scripts": {
    "build": "tsc",
    "test": "tsc && nyc mocha",
    "test-ci": "tsc && nyc mocha --reporter xunit --reporter-options output=./test-results.xml --reporter-options suiteName=$npm_package_name"
  },
  "main": "./lib/index.js",
  "typings": "./lib/index.d.ts",
  "dependencies": {
    "botbuilder": "^4.5.3",
    "botbuilder-core": "^4.5.3",
    "mssql": "^5.1.0"
  },
  "devDependencies": {
    "@istanbuljs/nyc-config-typescript": "^0.1.3",
    "@types/mssql": "^4.3.0",
    "@types/node": "^7.10.6",
    "mocha": "^6.2.0",
    "nyc": "^14.1.1",
    "rewire": "^4.0.1",
    "typescript": "^3.5.2"
  },
  "publishConfig": {
    "access": "public"
  }
}
