{
  "name": "@botbuildercommunity/text-recognizer-middleware",
  "version": "0.2.4",
  "description": "A collection of Bot Framework middleware components that uses Microsoft's recognizer text suite to parse inbound messages, and extract useful information.",
  "main": "./lib/index.js",
  "typings": "./lib/index.d.ts",
  "directories": {
    "lib": "lib"
  },
  "scripts": {
    "build": "tsc",
    "test": "tsc && mocha",
    "test-ci": "tsc && mocha --reporter xunit --reporter-options output=./mocha-test-results.xml --reporter-options suiteName=$npm_package_name"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/BotBuilderCommunity/botbuilder-community-js.git"
  },
  "keywords": [
    "chatbots",
    "bot framework",
    "bot builder",
    "text recognizers",
    "text recognizer suite",
    "middleware"
  ],
  "contributors": [
    "Bot Builder Community",
    "Michael Szul <michael@szul.us>"
  ],
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/BotBuilderCommunity/botbuilder-community-js/issues"
  },
  "homepage": "https://github.com/BotBuilderCommunity/botbuilder-community-js#readme",
  "dependencies": {
    "@microsoft/recognizers-text": "^1.1.4",
    "@microsoft/recognizers-text-number": "^1.1.4",
    "@microsoft/recognizers-text-number-with-unit": "^1.1.4",
    "@microsoft/recognizers-text-sequence": "^1.1.4",
    "botbuilder": "^4.5.3"
  },
  "devDependencies": {
    "mocha": "^6.2.0",
    "typescript": "^3.5.2"
  },
  "publishConfig": {
    "access": "public"
  }
}
