{
  "name": "@botbuildercommunity/adapter-console",
  "version": "1.0.1",
  "description": "Console adapter for the Bot Framework SDK v4.",
  "main": "./lib/index.js",
  "typings": "./lib/index.d.ts",
  "directories": {
    "lib": "lib"
  },
  "nyc": {
    "extends": "@istanbuljs/nyc-config-typescript",
    "all": true,
    "check-coverage": true,
    "lines": 0,
    "statements": 0,
    "functions": 0,
    "branches": 0,
    "exclude": [
      "**/*.d.ts",
      "**/*.test.js"
    ],
    "reporter": [
      "text",
      "cobertura",
      "html"
    ],
    "report-dir": "./.nyc_output/coverage"
  },
  "scripts": {
    "build": "tsc",
    "test": "tsc && nyc mocha",
    "test-ci": "tsc && nyc mocha --reporter xunit --reporter-options output=./mocha-test-results.xml --reporter-options suiteName=$npm_package_name"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/BotBuilderCommunity/botbuilder-community-js.git"
  },
  "keywords": [
    "chatbots",
    "bot framework",
    "bot builder",
    "console adapter",
    "adapters"
  ],
  "contributors": [
    "Bot Builder Community",
    "Michael Szul <michael@szul.us>"
  ],
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/BotBuilderCommunity/botbuilder-community-js/issues"
  },
  "homepage": "https://github.com/BotBuilderCommunity/botbuilder-community-js#readme",
  "dependencies": {
    "botbuilder": "^4.8.0"
  },
  "devDependencies": {
    "@istanbuljs/nyc-config-typescript": "^1.0.1",
    "mocha": "^7.1.1",
    "nyc": "^15.0.0",
    "sinon": "^9.0.1",
    "typescript": "^3.8.3"
  },
  "publishConfig": {
    "access": "public"
  }
}
