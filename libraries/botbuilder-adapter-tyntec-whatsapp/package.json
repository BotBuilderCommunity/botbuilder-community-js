{
  "name": "@botbuildercommunity/adapter-tyntec-whatsapp",
  "version": "1.0.0",
  "description": "A Microsoft Bot Framework adapter for handling connectivity with the WhatsApp channel in tyntec Conversations API.",
  "keywords": [
    "chatbots",
    "bot framework",
    "bot builder",
    "whatsapp adapter",
    "adapters",
    "whatsapp",
    "tyntec"
  ],
  "homepage": "https://github.com/BotBuilderCommunity/botbuilder-community-js#readme",
  "bugs": {
    "url": "https://github.com/BotBuilderCommunity/botbuilder-community-js/issues"
  },
  "license": "MIT",
  "contributors": [
    "Bot Builder Community"
  ],
  "main": "./lib/index.js",
  "typings": "./lib/index.d.ts",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/BotBuilderCommunity/botbuilder-community-js.git"
  },
  "scripts": {
    "build": "tsc",
    "test": "tsc && nyc mocha",
    "test-ci": "tsc && nyc mocha --reporter xunit --reporter-options output=./mocha-test-results.xml --reporter-options suiteName=$npm_package_name"
  },
  "dependencies": {
    "botbuilder": "^4.8.0"
  },
  "devDependencies": {
    "@istanbuljs/nyc-config-typescript": "^0.1.3",
    "axios": "^0.24.0",
    "mocha": "^9.0.3",
    "nyc": "^15.0.0",
    "typescript": "^4.3.5"
  },
  "peerDependencies": {
    "axios": "^0.24.0"
  },
  "engines": {
    "node": ">=14"
  },
  "publishConfig": {
    "access": "public"
  },
  "mocha": {
    "recursive": true
  },
  "nyc": {
    "extends": "@istanbuljs/nyc-config-typescript",
    "all": true,
    "check-coverage": true,
    "lines": 0,
    "statements": 0,
    "functions": 0,
    "branches": 0,
    "exclude": [
      "**/*.d.ts",
      "**/*.test.js"
    ],
    "reporter": [
      "text",
      "cobertura",
      "html"
    ],
    "report-dir": "./.nyc_output/coverage"
  }
}
